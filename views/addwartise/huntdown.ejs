    <script>
      var animation = bodymovin.loadAnimation({
        container: document.querySelector("#lottie1"),
        renderer: "svg",
        loop: true,
        autoplay: true,
        path: "https://assets3.lottiefiles.com/packages/lf20_lgk0wqey.json",
      });

      const startingMinutes = 15;
      let mintime = startingMinutes * 60; // Use 'time' consistently throughout
      let timerInterval;

      const countdownEl = document.getElementById("countdown");

      timerInterval = setInterval(updateCountdown, 1000);

      function updateCountdown() {
        const minutes = Math.floor(mintime / 60);
        let seconds = mintime % 60;

        // Add leading zero for single-digit seconds
        seconds = seconds < 10 ? "0" + seconds : seconds;

        countdownEl.innerHTML = `${minutes}:${seconds}`;

        if (mintime > 0) {
          mintime--; // Decrement the remaining time
        } else {
          clearInterval(timerInterval); // Stop the timer when it reaches 0
          autoSubmitTest();
        }
      }
      let testForm = document.querySelector("#testForm");
      function autoSubmitTest() {
        console.log("Time is up. Submitting form...");
        document.getElementById("testForm").submit();
        window.location.href = "/thanks";
      }

      function submitform() {
        HTMLFormElement.prototype.submit.call(testForm);
        console.log("Time is up. Submitting form...");
        window.location.href = "/thanks";
      }
    </script>

